package algorithms;
import java.awt.Point;

public class Arc {

	private Point left;
	private Point right;
	private double poid;
	
	public Arc(Point left,Point right)
	{
		this.left = left;
		this.right = right;
		this.poid = this.right.distance(this.left);
	}
	
	public double poid()
	{
		return this.poid;
	}
}
